<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY fibo-fnd-aap-agt "http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/">
	  <!ENTITY pfd "https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/PFD/" >
]>

<rdf:RDF xml:base="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/PFD/"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:pfd="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/PFD/"
     xmlns:fibo-fnd-aap-agt="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/">
    <owl:Ontology rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/PFD/">
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <dct:creator xml:lang="en">Jordan Faas-Bush</dct:creator>
        <rdfs:label xml:lang="en">Face Recognition Model Analyzer Ontology - Person, Face, and Demographic</rdfs:label>
        <skos:definition xml:lang="en">Machine learning allows us to learn a model for a given task such as facial recognition with a high degree of accuracy. However, after these models are generated they are often treated as black boxes and the limitations of a model are often unknown to the end user. The system developed for this use case will provide an intuitive interface to explore the limits of a facial recognition model by semantically integrating “smart” images(semantically describe who the image depicts and what Kumar [2] features the image exhibits) with classification results to discover common causes for misclassifications.

This sub-ontology is a section of the overall ontology focusing specifically on the features of a person such as their demographic and facial features that appear in their photos such as expression or nose shape.</skos:definition>
    </owl:Ontology>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- &owl;qualifiedCardinality -->

    <owl:AnnotationProperty rdf:about="&owl;qualifiedCardinality"/>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- &pfd;hasColor -->

    <owl:ObjectProperty rdf:about="&pfd;hasColor">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:label xml:lang="en">has color</rdfs:label>
        <skos:definition>relates an object to its color</skos:definition>
    </owl:ObjectProperty>



    <!-- &pfd;hasDemographic -->

    <owl:ObjectProperty rdf:about="&pfd;hasDemographic">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:label xml:lang="en">has demographic</rdfs:label>
        <skos:definition>relates the demograophic properties to a person who exhibts them</skos:definition>
    </owl:ObjectProperty>



    <!-- &pfd;hasShape -->

    <owl:ObjectProperty rdf:about="&pfd;hasShape">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:label xml:lang="en">has shape</rdfs:label>
        <skos:definition>relates an object to its shape</skos:definition>
    </owl:ObjectProperty>



    <!-- &pfd;hasSize -->

    <owl:ObjectProperty rdf:about="&pfd;hasSize">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:label xml:lang="en">has size</rdfs:label>
        <skos:definition>relates an object to its size</skos:definition>
    </owl:ObjectProperty>



    <!-- &pfd;hasVisualFeature -->

    <owl:ObjectProperty rdf:about="&pfd;hasVisualFeature">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:label xml:lang="en">has visual feature</rdfs:label>
        <skos:definition>relates a visual feature to the object exhibiting it</skos:definition>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- &pfd;hasRGB -->

    <owl:DatatypeProperty rdf:about="&pfd;hasRGB">
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:label xml:lang="en">has RGB</rdfs:label>
        <skos:definition xml:lang="en">relates the red, green, and blue values of a color ot the Color class</skos:definition>
    </owl:DatatypeProperty>



    <!-- &pfd;hasStrongNoseMouthLines -->

    <owl:DatatypeProperty rdf:about="&pfd;hasStrongNoseMouthLines">
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
        <rdfs:label xml:lang="en">has strong nose mouth lines</rdfs:label>
        <skos:definition xml:lang="en">the subject&apos;s nose mouth lines are highly visible</skos:definition>
    </owl:DatatypeProperty>



    <!-- &pfd;isAttractive -->

    <owl:DatatypeProperty rdf:about="&pfd;isAttractive">
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
        <rdfs:label xml:lang="en">is attractive</rdfs:label>
        <skos:definition xml:lang="en">a face that adheres to scientific beauty standards (golden ratio, horizontal face symmetry)</skos:definition>
    </owl:DatatypeProperty>



    <!-- &pfd;isBaggy -->

    <owl:DatatypeProperty rdf:about="&pfd;isBaggy">
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
        <rdfs:label xml:lang="en">is baggy</rdfs:label>
        <skos:definition xml:lang="en">relates whether an eye is baggy to eye features</skos:definition>
    </owl:DatatypeProperty>



    <!-- &pfd;isOpen -->

    <owl:DatatypeProperty rdf:about="&pfd;isOpen">
        <rdfs:label xml:lang="en">is open</rdfs:label>
        <skos:definition xml:lang="en">relates whether an eye is open to eye features</skos:definition>
    </owl:DatatypeProperty>



    <!-- &pfd;teethVisable -->

    <owl:DatatypeProperty rdf:about="&pfd;teethVisable">
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
        <rdfs:label xml:lang="en">teeth visable</rdfs:label>
        <skos:definition xml:lang="en">relates the visablity of teeth to features of the mouth</skos:definition>
    </owl:DatatypeProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- &pfd;AgeRange -->

    <owl:Class rdf:about="&pfd;AgeRange">
        <rdfs:subClassOf rdf:resource="&pfd;Demographic"/>
        <rdfs:label xml:lang="en">Age Range</rdfs:label>
        <skos:definition xml:lang="en">how many years old a person is</skos:definition>
    </owl:Class>



    <!-- &pfd;Baby -->

    <owl:Class rdf:about="&pfd;Baby">
        <rdfs:subClassOf rdf:resource="&pfd;AgeRange"/>
    </owl:Class>



    <!-- &pfd;Cheek -->

    <owl:Class rdf:about="&pfd;Cheek">
        <rdfs:label xml:lang="en">Cheek</rdfs:label>
        <skos:definition xml:lang="en">either side of the face below the eyes</skos:definition>
    </owl:Class>



    <!-- &pfd;Child -->

    <owl:Class rdf:about="&pfd;Child">
        <rdfs:subClassOf rdf:resource="&pfd;AgeRange"/>
    </owl:Class>



    <!-- &pfd;Chin -->

    <owl:Class rdf:about="&pfd;Chin">
        <rdfs:label xml:lang="en">Chin</rdfs:label>
        <skos:definition xml:lang="en">the protruding part of the face below the mouth, formed by the apex of the lower jaw</skos:definition>
    </owl:Class>



    <!-- &pfd;Chubby -->

    <owl:Class rdf:about="&pfd;Chubby">
        <rdfs:subClassOf rdf:resource="&pfd;WeightRange"/>
    </owl:Class>



    <!-- &pfd;Color -->

    <owl:Class rdf:about="&pfd;Color">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasRGB"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Color</rdfs:label>
        <skos:definition xml:lang="en">a phenomenon of light (such as red, brown, pink, or gray) or visual perception that enables one to differentiate otherwise identical objects</skos:definition>
    </owl:Class>



    <!-- &pfd;Demographic -->

    <owl:Class rdf:about="&pfd;Demographic">
        <rdfs:label>Demographic</rdfs:label>
        <skos:definition>statistical data relating to the population and particular groups within it</skos:definition>
    </owl:Class>



    <!-- &pfd;Ethnicity -->

    <owl:Class rdf:about="&pfd;Ethnicity">
        <rdfs:subClassOf rdf:resource="&pfd;Demographic"/>
        <rdfs:label xml:lang="en">Ethnicity</rdfs:label>
        <skos:definition xml:lang="en">the ethnicity of the individual in the photo</skos:definition>
    </owl:Class>



    <!-- &pfd;Eye -->

    <owl:Class rdf:about="&pfd;Eye">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasColor"/>
                <owl:someValuesFrom rdf:resource="&pfd;EyeColor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Eye</rdfs:label>
        <skos:definition xml:lang="en">each of a pair of globular organs in the head through which people and vertebrate animals see, the visible part typically appearing almond-shaped in animals with eyelids.</skos:definition>
    </owl:Class>



    <!-- &pfd;EyeColor -->

    <owl:Class rdf:about="&pfd;EyeColor">
        <rdfs:subClassOf rdf:resource="&pfd;Color"/>
        <rdfs:label xml:lang="en">Eye Color</rdfs:label>
        <skos:definition xml:lang="en">the color of an eye</skos:definition>
    </owl:Class>



    <!-- &pfd;Face -->

    <owl:Class rdf:about="&pfd;Face">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasShape"/>
                <owl:someValuesFrom rdf:resource="&pfd;FaceShape"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;Cheek"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;Chin"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;Eye"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;FacialExpression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;ForheadVisibility"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;Mouth"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;Nose"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;Skin"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;isBaggy"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;isOpen"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Face</rdfs:label>
        <skos:definition xml:lang="en">the front part of a person&apos;s head from the forehead to the chin, or the corresponding part in an animal</skos:definition>
    </owl:Class>



    <!-- &pfd;FaceShape -->

    <owl:Class rdf:about="&pfd;FaceShape">
        <rdfs:label xml:lang="en">Face Shape</rdfs:label>
        <skos:definition xml:lang="en">the shape of the subject&apos;s face</skos:definition>
    </owl:Class>



    <!-- &pfd;FacialExpression -->

    <owl:Class rdf:about="&pfd;FacialExpression">
        <rdfs:label xml:lang="en">Facial Expression</rdfs:label>
        <skos:definition xml:lang="en">the expression the subject is making in the photo</skos:definition>
    </owl:Class>



    <!-- &pfd;Feminine -->

    <owl:Class rdf:about="&pfd;Feminine">
        <rdfs:subClassOf rdf:resource="&pfd;GenderExpression"/>
        <owl:disjointWith rdf:resource="&pfd;Masculine"/>
    </owl:Class>



    <!-- &pfd;Finish -->

    <owl:Class rdf:about="&pfd;Finish">
        <rdfs:label xml:lang="en">Finish</rdfs:label>
        <skos:definition xml:lang="en">the refelectivness of a material</skos:definition>
    </owl:Class>



    <!-- &pfd;ForheadVisibility -->

    <owl:Class rdf:about="&pfd;ForheadVisibility">
        <rdfs:label xml:lang="en">Forehead Visibility</rdfs:label>
        <skos:definition xml:lang="en">amount of the subject&apos;s forehead that can be seen in the photo</skos:definition>
    </owl:Class>



    <!-- &pfd;Frowning -->

    <owl:Class rdf:about="&pfd;Frowning">
        <rdfs:subClassOf rdf:resource="&pfd;FacialExpression"/>
        <owl:disjointWith rdf:resource="&pfd;Smiling"/>
    </owl:Class>



    <!-- &pfd;GenderExpression -->

    <owl:Class rdf:about="&pfd;GenderExpression">
        <rdfs:subClassOf rdf:resource="&pfd;Demographic"/>
        <rdfs:label xml:lang="en">Gender Expression</rdfs:label>
        <skos:definition xml:lang="en">whether a person is male or female</skos:definition>
    </owl:Class>



    <!-- &pfd;HighCheekbones -->

    <owl:Class rdf:about="&pfd;HighCheekbones">
        <rdfs:subClassOf rdf:resource="&pfd;Cheek"/>
        <owl:disjointWith rdf:resource="&pfd;RosyCheeks"/>
        <rdfs:label xml:lang="en">High Cheekbones</rdfs:label>
        <skos:definition xml:lang="en">the subject&apos;s cheeks are labeled as high</skos:definition>
    </owl:Class>



    <!-- &pfd;Masculine -->

    <owl:Class rdf:about="&pfd;Masculine">
        <rdfs:subClassOf rdf:resource="&pfd;GenderExpression"/>
    </owl:Class>



    <!-- &pfd;MiddleAged -->

    <owl:Class rdf:about="&pfd;MiddleAged">
        <rdfs:subClassOf rdf:resource="&pfd;AgeRange"/>
        <rdfs:label xml:lang="en">Middle Aged</rdfs:label>
        <skos:definition xml:lang="en">person from age 45 to 64</skos:definition>
    </owl:Class>



    <!-- &pfd;Mouth -->

    <owl:Class rdf:about="&pfd;Mouth">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasSize"/>
                <owl:someValuesFrom rdf:resource="&pfd;Size"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;MouthOpenState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Mouth</rdfs:label>
        <skos:definition xml:lang="en">opening in the lower part of the human face, surrounded by the lips</skos:definition>
    </owl:Class>



    <!-- &pfd;MouthOpenState -->

    <owl:Class rdf:about="&pfd;MouthOpenState">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;teethVisable"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Mouth Open State</rdfs:label>
        <skos:definition xml:lang="en">mouth shape</skos:definition>
    </owl:Class>



    <!-- &pfd;NarrowEyes -->

    <owl:Class rdf:about="&pfd;NarrowEyes">
        <rdfs:subClassOf rdf:resource="&pfd;FacialExpression"/>
        <rdfs:label xml:lang="en">Narrow Eyes</rdfs:label>
        <skos:definition xml:lang="en">the subject&apos;s eyes appear narrow</skos:definition>
    </owl:Class>



    <!-- &pfd;Nose -->

    <owl:Class rdf:about="&pfd;Nose">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasShape"/>
                <owl:someValuesFrom rdf:resource="&pfd;NoseShape"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasSize"/>
                <owl:someValuesFrom rdf:resource="&pfd;Size"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Nose</rdfs:label>
        <skos:definition xml:lang="en">part of the face projecting above the mouth on the face of a person or animal, containing the nostrils and used for breathing and smelling</skos:definition>
    </owl:Class>



    <!-- &pfd;NoseShape -->

    <owl:Class rdf:about="&pfd;NoseShape">
        <rdfs:label xml:lang="en">NoseShape</rdfs:label>
        <skos:definition xml:lang="en">having a pointy tip or end</skos:definition>
    </owl:Class>



    <!-- &pfd;Person -->

    <owl:Class rdf:about="&pfd;Person">
        <rdfs:subClassOf rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasDemographic"/>
                <owl:someValuesFrom rdf:resource="&pfd;Demographic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;Face"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Person</rdfs:label>
        <skos:definition xml:lang="en">human being</skos:definition>
    </owl:Class>



    <!-- &pfd;RosyCheeks -->

    <owl:Class rdf:about="&pfd;RosyCheeks">
        <rdfs:subClassOf rdf:resource="&pfd;Cheek"/>
        <rdfs:label xml:lang="en">Rosy Cheeks</rdfs:label>
        <skos:definition xml:lang="en">the subject&apos;s cheeks appear rosy</skos:definition>
    </owl:Class>



    <!-- &pfd;Senior -->

    <owl:Class rdf:about="&pfd;Senior">
        <rdfs:subClassOf rdf:resource="&pfd;AgeRange"/>
    </owl:Class>



    <!-- &pfd;Size -->

    <owl:Class rdf:about="&pfd;Size">
        <rdfs:label xml:lang="en">Size</rdfs:label>
        <skos:definition xml:lang="en">how large an object is</skos:definition>
    </owl:Class>



    <!-- &pfd;Skin -->

    <owl:Class rdf:about="&pfd;Skin">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;Finish"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pfd;hasVisualFeature"/>
                <owl:someValuesFrom rdf:resource="&pfd;SkinTone"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Skin</rdfs:label>
        <skos:definition xml:lang="en">the epidermus of the main subject of the photo</skos:definition>
    </owl:Class>



    <!-- &pfd;SkinTone -->

    <owl:Class rdf:about="&pfd;SkinTone">
        <rdfs:label xml:lang="en">Skin Tone</rdfs:label>
        <skos:definition xml:lang="en">the realtive color</skos:definition>
    </owl:Class>



    <!-- &pfd;Smiling -->

    <owl:Class rdf:about="&pfd;Smiling">
        <rdfs:subClassOf rdf:resource="&pfd;FacialExpression"/>
    </owl:Class>



    <!-- &pfd;WeightRange -->

    <owl:Class rdf:about="&pfd;WeightRange">
        <rdfs:subClassOf rdf:resource="&pfd;Demographic"/>
        <rdfs:label xml:lang="en">Weight Range</rdfs:label>
        <skos:definition xml:lang="en">subjective labels given by the tags for LFW</skos:definition>
    </owl:Class>



    <!-- &pfd;Youth -->

    <owl:Class rdf:about="&pfd;Youth">
        <rdfs:subClassOf rdf:resource="&pfd;AgeRange"/>
    </owl:Class>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- &pfd;Asian -->

    <owl:NamedIndividual rdf:about="&pfd;Asian">
        <rdf:type rdf:resource="&pfd;Ethnicity"/>
    </owl:NamedIndividual>



    <!-- &pfd;Baby -->

    <owl:NamedIndividual rdf:about="&pfd;Baby">
        <rdf:type rdf:resource="&pfd;Baby"/>
    </owl:NamedIndividual>



    <!-- &pfd;Big -->

    <owl:NamedIndividual rdf:about="&pfd;Big">
        <rdf:type rdf:resource="&pfd;Size"/>
        <rdfs:label xml:lang="en">Big</rdfs:label>
        <skos:definition xml:lang="en">categorized as &quot;big&quot; by the LFW tags</skos:definition>
    </owl:NamedIndividual>



    <!-- &pfd;Black -->

    <owl:NamedIndividual rdf:about="&pfd;Black">
        <rdf:type rdf:resource="&pfd;Ethnicity"/>
    </owl:NamedIndividual>



    <!-- &pfd;BrownEyeColor -->

    <owl:NamedIndividual rdf:about="&pfd;Brown">
        <rdf:type rdf:resource="&pfd;EyeColor"/>
        <rdfs:label xml:lang="en">Brown Eye Color</rdfs:label>
        <skos:definition xml:lang="en">the eye has a brown color</skos:definition>
    </owl:NamedIndividual>



    <!-- &pfd;Child -->

    <owl:NamedIndividual rdf:about="&pfd;Child">
        <rdf:type rdf:resource="&pfd;Child"/>
    </owl:NamedIndividual>



