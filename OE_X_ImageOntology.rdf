    <owl:Ontology rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/">
        <owl:versionIRI rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/1.0.1"/>
        <owl:imports rdf:resource="http://www.imagesnippets.com/lio/lio.owl"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <dct:creator>Alexander Schwartzberg</dct:creator>
        <dct:modified>11/22/2018</dct:modified>
        <rdfs:comment>Added owl:VersionIRI, owlVersionInfo, and owlPriorVersion metadata.</rdfs:comment>
        <rdfs:label xml:lang="en">Face Recognition Model Analyzer Ontology - Image Sub-Ontology</rdfs:label>
        <owl:priorVersion>https://raw.githubusercontent.com/FRMA-Ontology/Ontology/9ad9704c913d0aee68056473efa1e3574a8808a8/IMG.rdf</owl:priorVersion>
        <owl:versionInfo>Modified on 11/17/2018</owl:versionInfo>
        <skos:definition xml:lang="en">Image Ontology enables the FRMA system to classify the properties of different image files.</skos:definition>
    </owl:Ontology>


