     -->




    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/fidelityDescribedBy -->

    <owl:ObjectProperty rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/fidelityDescribedBy">
        <rdfs:label xml:lang="en">fidelity described by</rdfs:label>
        <skos:definition xml:lang="en">the fidelity of this image</skos:definition>
    </owl:ObjectProperty>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/isStoredAs -->

    <owl:ObjectProperty rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/isStoredAs">
        <owl:inverseOf rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/stores"/>
        <rdfs:domain rdf:resource="https://w3id.org/lio/v1#Image"/>
        <rdfs:range rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFile"/>
        <rdfs:label xml:lang="en">is stored as</rdfs:label>
        <skos:definition xml:lang="en">links a given image to a file in which it is stored</skos:definition>
    </owl:ObjectProperty>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/lightingDescribedBy -->

    <owl:ObjectProperty rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/lightingDescribedBy">
        <rdfs:label xml:lang="en">lighting described by</rdfs:label>
        <skos:definition xml:lang="en">the lighting characteristics of this image</skos:definition>
    </owl:ObjectProperty>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/stores -->

    <owl:ObjectProperty rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/stores">
        <rdfs:domain rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFile"/>
        <rdfs:range rdf:resource="https://w3id.org/lio/v1#Image"/>
        <rdfs:label xml:lang="en">stores</rdfs:label>
        <skos:definition xml:lang="en">links a given image file to the image that it contains</skos:definition>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/hasFileExtension -->

    <owl:DatatypeProperty rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/hasFileExtension">
        <rdfs:domain rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">has file extension</rdfs:label>
        <skos:definition xml:lang="en">the extension of the image file</skos:definition>
        <skos:example rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Example extensions include jpg, jpeg, gif, png, which have corresponding file format types and characteristics.</skos:example>
    </owl:DatatypeProperty>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/hasFilePath -->

    <owl:DatatypeProperty rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/hasFilePath">
        <rdfs:domain rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:label xml:lang="en">has file path</rdfs:label>
        <skos:definition xml:lang="en">an absolute file path for the image file as stored on a local or network file system</skos:definition>
    </owl:DatatypeProperty>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/hasRGB -->

    <owl:DatatypeProperty rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/hasRGB">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">has RGB</rdfs:label>
        <skos:definition xml:lang="en">relates the red, green, and blue values of a color ot the Color class</skos:definition>
    </owl:DatatypeProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/BalancedLightingVariation -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/BalancedLightingVariation">
        <rdfs:subClassOf rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/LightingVariation"/>
        <owl:disjointWith rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/HarshLightingVariation"/>
        <owl:disjointWith rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/SoftLightingVariation"/>
        <rdfs:label>balanced lighting variation</rdfs:label>
        <skos:definition>both small light source in close proximity relative to the subject and large light source in distant proximity relative to the subject</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/BlackAndWhiteImage -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/BlackAndWhiteImage">
        <rdfs:subClassOf rdf:resource="https://w3id.org/lio/v1#Image"/>
        <rdfs:label xml:lang="en">black and white image</rdfs:label>
        <skos:definition xml:lang="en">an image having, showing, or producing pictures that do not have colors except for black, white, and shades of gray</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/BlurryImageFidelity -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/BlurryImageFidelity">
        <rdfs:subClassOf rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFidelity"/>
        <owl:disjointWith rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/SharpImageFidelity"/>
        <rdfs:label xml:lang="en">blurry image fidelity</rdfs:label>
        <skos:definition xml:lang="en">lower quality image showing signs of visible distortion or information loss</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/CandidImage -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/CandidImage">
        <rdfs:subClassOf rdf:resource="https://w3id.org/lio/v1#Image"/>
        <rdfs:label>candid image</rdfs:label>
        <skos:definition>a photo in which the subject is acting naturally or spontaneously</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ColorImage -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ColorImage">
        <rdfs:subClassOf rdf:resource="https://w3id.org/lio/v1#Image"/>
        <rdfs:label xml:lang="en">color image</rdfs:label>
        <skos:definition xml:lang="en">an image produces useing media capable of reproducing colors</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/DepictedBackground -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/DepictedBackground">
        <rdfs:label>depicted background</rdfs:label>
        <skos:definition xml:lang="en">the ground or parts, as of a scene, situated in the rear</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/FlashLightingVariation -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/FlashLightingVariation">
        <rdfs:subClassOf rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/HarshLightingVariation"/>
        <rdfs:label>flash lighting variation</rdfs:label>
        <skos:definition>whether or not the system classifies a photo as using camera flash to illuminate the subject</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/HarshLightingVariation -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/HarshLightingVariation">
        <rdfs:subClassOf rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/LightingVariation"/>
        <owl:disjointWith rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/SoftLightingVariation"/>
        <rdfs:label>harsh lighting variation</rdfs:label>
        <skos:definition>small light source in close proximity relative to the subject</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFidelity -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFidelity">
        <rdfs:label xml:lang="en">image fidelity</rdfs:label>
        <skos:definition xml:lang="en">the measurable quality of an image</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFile -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFile">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/stores"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/lio/v1#Image"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">image file</rdfs:label>
        <skos:definition xml:lang="en">a digital version of a photograph, stored in any of a number of standard formats that can be rasterized for display purposes</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/Indoors -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/Indoors">
        <rdfs:subClassOf rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/DepictedBackground"/>
        <owl:disjointWith rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/Outdoors"/>
        <rdfs:label xml:lang="en">indoors</rdfs:label>
        <skos:definition xml:lang="en">whether or not the image was taken indoors</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/LightingVariation -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/LightingVariation">
        <rdfs:label>lighting variation</rdfs:label>
        <skos:definition>images have differening levels and locations of brightness</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/OptimalConditionImage -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/OptimalConditionImage">
        <rdfs:label xml:lang="en">optimal condition image</rdfs:label>
        <skos:definition xml:lang="en">the best possible image for recognition tasks</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/Outdoors -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/Outdoors">
        <rdfs:subClassOf rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/DepictedBackground"/>
        <rdfs:label>outdoors</rdfs:label>
        <skos:definition xml:lang="en">whether or not the image was taken outdoors</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/PosedImage -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/PosedImage">
        <rdfs:subClassOf rdf:resource="https://w3id.org/lio/v1#Image"/>
        <rdfs:label>posed image</rdfs:label>
        <skos:definition>assumes a posture or attitude usually for artistic purposes</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/SharpImageFidelity -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/SharpImageFidelity">
        <rdfs:subClassOf rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFidelity"/>
        <rdfs:label xml:lang="en">sharp image fidelity</rdfs:label>
        <skos:definition xml:lang="en">high quality image showing little to no signs of visible distortion or information loss</skos:definition>
    </owl:Class>



    <!-- https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/SoftLightingVariation -->

    <owl:Class rdf:about="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/SoftLightingVariation">
        <rdfs:subClassOf rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/LightingVariation"/>
        <rdfs:label>soft lighting variation</rdfs:label>
        <skos:definition>large light source in distant proximity relative to the subject</skos:definition>
    </owl:Class>



    <!-- https://w3id.org/lio/v1#Image -->

    <rdf:Description rdf:about="https://w3id.org/lio/v1#Image">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/lio#hasDepictedBackground"/>
                <owl:someValuesFrom rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/DepictedBackground"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/fidelityDescribedBy"/>
                <owl:someValuesFrom rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFidelity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/isStoredAs"/>
                <owl:someValuesFrom rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/ImageFile"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/lightingDescribedBy"/>
                <owl:someValuesFrom rdf:resource="https://tw.rpi.edu/web/Courses/Ontologies/2018/FRMA/ImageOntology/LightingVariation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/lio/v1#depicts"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/lio/v1#PictorialElement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>
    <rdf:Description>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </rdf:Description>
</rdf:RDF>



